@using Daemon.RazorUI.Color

@namespace Daemon.RazorUI.Icons

@inherits DaemonBaseIcon

<svg 
    xmlns="http://www.w3.org/2000/svg" 
    fill="none" 
    viewBox="0 0 24 24" 
    stroke-width="1.5" 
    stroke="currentColor" 
    class=@(Props.ToClass())
>
  @ChildContent
</svg>

@code {
    [Parameter, EditorRequired]
    public RenderFragment? ChildContent { get; set; }
}